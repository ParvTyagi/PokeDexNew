generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Pokemon {
  id Int @id @default(autoincrement())
  name String
  type String
  photo Bytes?
  height String
  weight String
}

model Season {
  id Int @id @default(autoincrement())
  image String
  name String
  episodes String
  year String
  desc String
}

model Movies {
  id Int @id @default(autoincrement())
  image String
  name String
  year String
  desc String
}